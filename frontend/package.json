{
  "name": "sistem-absensi-akademik-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.local -- next dev",
    "dev:full": "concurrently \"npm run pdf-server\" \"npm run dev\"",
    "pdf-server": "dotenv -e .env.local -- node pdf-server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:push": "dotenv -e .env.local -- prisma db push",
    "prisma:seed": "dotenv -e .env.local -- ts-node -r tsconfig-paths/register prisma/seed.ts",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^7.0.9",
    "@types/qrcode": "^1.5.6",
    "axios": "^1.6.5",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.2.1",
    "goober": "^2.1.18",
    "html5-qrcode": "^2.3.8",
    "next": "14.0.4",
    "nodemailer": "^7.0.13",
    "pdfmake": "^0.2.10",
    "prisma": "^5.22.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "sweetalert2": "^11.26.18",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.23",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^13.0.1",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "mysql2": "^3.16.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
}
